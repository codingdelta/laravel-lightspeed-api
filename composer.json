{
    "name": "gunharth/laravel-lightspeed-api",
    "description": "A Laravel wrapper for the Lightspeed Seoshop API",
    "keywords": [
        "laravel",
        "lightspeed",
        "seoshop",
        "api",
        "laravel-lightspeed-api"
    ],
    "homepage": "https://github.com/gunharth/laravel-lightspeed-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Gunharth Randolf",
            "email": "hello@gunharth.io",
            "homepage": "https://gunharth.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/support": "^5.2.8",
        "seoshop/seoshop-php": "~1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3.4",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Gunharth\\Lightspeed\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gunharth\\Lightspeed\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
